<query id="insertFileLog" action="insert">
    <tables>
        <table name="file_logs"/>
    </tables>
    <columns>
        <column name="file_log_srl" var="file_log_srl" notnull="notnull"/>
        <column name="file_srl" var="file_srl" notnull="notnull"/>
        <column name="member_srl" var="member_srl" default="0"/>
        <column name="action" var="action" notnull="notnull"/>
        <column name="log_date" var="log_date" default="curdate()"/>
        <column name="ipaddress" var="ipaddress" default="ipaddress()" />
    </columns>
</query>
